                        "sourcedId": {
                          "type": "string",
                          "format": "uuid"
                        },
                        "title": {
                          "type": "string"
                        },
                        "uri": {
                          "type": "string",
                          "format": "uri"
                        }
                      },
                      "required": ["sourcedId", "title", "uri"]
                    },
                    "CFAssociationGroupingURI": {
                      "type": ["object", "null"],
                      "properties": {
                        "sourcedId": {
                          "type": "string",
                          "format": "uuid"
                        },
                        "title": {
                          "type": "string"
                        },
                        "uri": {
                          "type": "string",
                          "format": "uri"
                        }
                      },
                      "required": ["sourcedId", "title", "uri"]
                    },
                    "lastChangeDateTime": {
                      "type": "string",
                      "format": "date-time"
