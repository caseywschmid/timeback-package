        "description": "Successfully retrieved agents for user",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "agents": {
                  "type": "array",
                  "items": { "$ref": "#/components/schemas/User" }
                }
              },
              "required": ["agents"]
            }
          }
        }
      },
