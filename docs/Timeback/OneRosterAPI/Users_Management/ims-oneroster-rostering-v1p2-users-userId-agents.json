  "get": {
    "operationId": "getAgents",
    "summary": "Get agents for a user",
    "description": "Get agents for a user",
    "tags": ["Users Management"],
    "parameters": [
      {
        "name": "userId",
        "description": "The sourcedId of the user",
        "in": "path",
        "required": true,
        "schema": { "type": "string" }
